<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".MainActivity">

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/voipFirst"
        android:layout_width="274dp"
        android:layout_height="84dp"
        android:layout_marginTop="57dp"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="37dp"
        app:oe_drawableLeft="@drawable/phone_call_icon"
        app:oe_paddingLeft="50dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_text="영상통화"
        app:oe_textSize="30sp"
        app:oe_paddingRight="50dp"
        app:oe_drawablePadding="20dp"
        app:oe_textColor="@color/selector_button_text"
        app:oe_backgroundImg="@drawable/confirm_button"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/voipSecond"
        android:layout_width="274dp"
        android:layout_height="84dp"
        android:layout_marginTop="43dp"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="37dp"
        app:oe_drawableLeft="@drawable/phone_call_icon"
        app:oe_paddingLeft="50dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_text="영상통화"
        app:oe_textSize="30sp"
        app:oe_paddingRight="50dp"
        app:oe_drawablePadding="20dp"
        app:oe_textColor="@color/selector_button_text"
        app:oe_backgroundImg="@drawable/confirm_button"
        app:layout_constraintTop_toBottomOf="@id/voipFirst"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/voipThird"
        android:layout_width="274dp"
        android:layout_height="84dp"
        android:layout_marginTop="43dp"
        android:layout_marginRight="37dp"
        app:oe_drawableLeft="@drawable/phone_call_icon"
        app:oe_paddingLeft="50dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_text="영상통화"
        app:oe_textSize="30sp"
        app:oe_paddingRight="50dp"
        app:oe_drawablePadding="20dp"
        app:oe_textColor="@color/selector_button_text"
        app:oe_backgroundImg="@drawable/confirm_button"
        app:layout_constraintTop_toBottomOf="@id/voipSecond"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/all"
        android:layout_width="211dp"
        android:layout_height="89dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="54dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_isShadow="false"
        app:oe_text="전체메뉴"
        app:oe_textSize="30sp"
        app:oe_fontStyle="@font/notosans_m"
        app:oe_paddingLeft="10dp"
        app:oe_paddingRight="31dp"
        app:oe_backgroundImg="@drawable/basic_button_black"
        app:oe_drawableRight="@drawable/all_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/training"
        android:layout_width="211dp"
        android:layout_height="89dp"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="54dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_isShadow="false"
        app:oe_text="두뇌훈련"
        app:oe_textSize="30sp"
        app:oe_fontStyle="@font/notosans_m"
        app:oe_paddingLeft="25dp"
        app:oe_paddingRight="28dp"
        app:oe_drawablePadding="5dp"
        app:oe_backgroundImg="@drawable/basic_button_green"
        app:oe_drawableRight="@drawable/training_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/all" />

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/enjoyLife"
        android:layout_width="211dp"
        android:layout_height="89dp"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="54dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_isShadow="false"
        app:oe_text="즐거운생활"
        app:oe_textSize="30sp"
        app:oe_paddingLeft="5dp"
        app:oe_paddingRight="13dp"
        app:oe_fontStyle="@font/notosans_m"
        app:oe_backgroundImg="@drawable/basic_button_orange"
        app:oe_drawableRight="@drawable/enjoy_life_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/training" />

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/contacts"
        android:layout_width="211dp"
        android:layout_height="89dp"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="54dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_isShadow="false"
        app:oe_text="소통"
        app:oe_textSize="30sp"
        app:oe_paddingLeft="10dp"
        app:oe_paddingRight="43dp"
        app:oe_fontStyle="@font/notosans_m"
        app:oe_backgroundImg="@drawable/confirm_button"
        app:oe_drawableRight="@drawable/contacts_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/enjoyLife" />

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:id="@+id/healthCare"
        android:layout_width="211dp"
        android:layout_height="89dp"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="54dp"
        app:oe_isKeepshadowMargin="false"
        app:oe_isShadow="false"
        app:oe_text="건강관리"
        app:oe_textSize="30sp"
        app:oe_paddingLeft="10dp"
        app:oe_paddingRight="20dp"
        app:oe_fontStyle="@font/notosans_m"
        app:oe_backgroundImg="@drawable/reject_button"
        app:oe_drawableRight="@drawable/health_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/contacts" />

    <com.opusone.leanon.testcumstomview.OoFlatButton
        android:layout_width="188dp"
        android:layout_height="188dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:oe_paddingTop="30dp"
        app:oe_drawableTop="@drawable/poweroff_icon"
        app:oe_text="린온종료"
        app:oe_fontStyle="@font/notosans_m"
        app:oe_textColor="@color/selector_button_text_black"
        app:oe_textSize="26sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


</androidx.constraintlayout.widget.ConstraintLayout>